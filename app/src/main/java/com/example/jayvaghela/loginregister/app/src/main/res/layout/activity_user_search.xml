<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context=".app.src.main.java.com.example.jayvaghela.loginregister.UI.UserSearch">

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView9"
        android:src="@drawable/wty"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView10"
        android:src="@drawable/sn"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/imageView9"
        android:layout_alignParentLeft="true" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Search University"
        android:id="@+id/bSU"
        android:textColor="#ef196f"
        android:background="@android:drawable/editbox_background_normal"
        android:layout_centerVertical="true"
        android:layout_alignRight="@+id/imageView10"
        android:layout_alignEnd="@+id/imageView10" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Search Module"
        android:id="@+id/bM"
        android:layout_below="@+id/bSU"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_alignLeft="@+id/bSU"
        android:layout_alignStart="@+id/bSU"
        android:textColor="#ef196f"
        android:background="@android:drawable/editbox_background_normal" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Search Course"
        android:id="@+id/bB"
        android:layout_below="@+id/bM"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_alignLeft="@+id/bM"
        android:layout_alignStart="@+id/bM"
        android:textColor="#ef196f"
        android:background="@android:drawable/editbox_background_normal" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/etUniversitySearch"
        android:layout_alignTop="@+id/bSU"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_toLeftOf="@+id/bSU"
        android:layout_toStartOf="@+id/bSU" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/etModuleSearch"
        android:layout_above="@+id/bB"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_toLeftOf="@+id/bM"
        android:layout_toStartOf="@+id/bM" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/etCourseSearch"
        android:layout_below="@+id/bM"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignBottom="@+id/bB"
        android:layout_toLeftOf="@+id/bM"
        android:layout_toStartOf="@+id/bM" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/qsearch"
        android:src="@drawable/mg"
        android:layout_below="@+id/bB"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Search Username"
        android:id="@+id/button"
        android:textColor="#ef196f"
        android:background="@android:drawable/editbox_background_normal"
        android:layout_alignTop="@+id/etUsernameSearch"
        android:layout_toRightOf="@+id/etUniversitySearch"
        android:layout_toEndOf="@+id/etUniversitySearch" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/etUsernameSearch"
        android:layout_above="@+id/etUniversitySearch"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_toLeftOf="@+id/button"
        android:layout_toStartOf="@+id/button" />

</RelativeLayout>
